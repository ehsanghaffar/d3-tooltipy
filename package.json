{
  "name": "d3-tooltipy",
  "version": "0.0.1",
  "description": "Tooltip generator for d3 svg visualization library",
  "keywords": [
    "d3",
    "tooltip"
  ],
  "homepage": "https://github.com/ehsanghaffarii/d3-tooltipy",
  "bugs": {
    "url": "https://github.com/ehsanghaffarii/issues"
  },
  "files": [
    "dist/",
    "index.js"
  ],
  "license": "MIT",
  "author": "Ehsan Ghaffar <ghafari.5000@gmail.com>",
  "main": "dist/index.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ehsanghaffarii"
  },
  "scripts": {
    "circle:lint": "npm run -s lint -- --max-warnings 0 -f junit -o $CIRCLE_TEST_REPORTS/eslint/junit.xml",
    "lint": "eslint . --ignore-path .gitignore",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "python3 -m http.server",
    "prepare": "rollup -c rollup.conf.js"
  },
  "dependencies": {
    "d3-collection": "^1.0.4",
    "d3-selection": "^1.3.0"
  },
  "devDependencies": {
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.14.0",
    "rollup": "^0.58.2"
  },
  "engines": {
    "node": ">=4.2.6"
  }
}
